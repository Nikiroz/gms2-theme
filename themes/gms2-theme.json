{
  "$schema": "vscode://schemas/color-theme",
  "name": "GameMaker Studio 2 Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "macro": "#ff5b5b",
    "enum": "#ff8080",
    "enumMember": "#ff8080",
    "variable.local": "#fff899",
    "parameter.local": "#fff899",
    "variable.instance": "#b2b1ff",
    "variable.defaultLibrary": "#58e55a",
    "property.defaultLibrary": "#58e55a",
    "property.defaultLibrary.source.gml": "#58e55a",
    "variable.asset": "#ff8080",
    "variable.constant": "#ff8080",
    "constant": "#ff8080",
  },
  "colors": {
    "editor.background": "#222222",
    "foreground": "#c0c0c0",
    "editor.foreground": "#c0c0c0",
    "sideBar.background": "#222222",
    "sideBar.foreground": "#c0c0c0",
    "editor.foldBackground": "#00000000",
    "editor.selectionBackground": "#78848a",
    "editor.inactiveSelectionBackground": "#78848aee",
    "editorLineNumber.foreground": "#40c3f4",
    "editorLineNumber.activeForeground": "#40c3f4",
    "editor.lineHighlightBackground": "#444444",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveForeground": "#ffffff80",
    "tab.activeBackground": "#1f1f1f",
    "tab.inactiveBackground": "#181818",
    "editorCursor.foreground": "#c0c0c0",
    "editorGutter.background": "#2b343b",
    "editorBracketMatch.border": "#c0c0c0",
    "editorBracketMatch.background": "#00000000"
  },
  "tokenColors": [
    {
      "scope": [
        "variable.language",
        "support.variable",
        "support.variable.property",
        "support.constant.variable"
      ],
      "settings": {
        "foreground": "#58e55a"
      }
    },
    {
      "scope": [
        "string",
        "string.tag",
        "string.value",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "variable.other.constant",
        "variable.other.enummember",
        "constant.language.gml",
      ],
      "settings": {
        "foreground": "#ff8080"
      }
    },
    {
      "scope": [
        "keyword.operator.arithmetic.gml",
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "punctuation.section.parens.begin",
        "punctuation.section.parens.end",
        "punctuation.section.brackets.begin",
        "punctuation.section.brackets.end",
        "punctuation.definition.parameters.begin",
        "punctuation.definition.parameters.end",
        "punctuation.definition.array.begin",
        "punctuation.definition.array.end",
        "meta.brace.round",
        "meta.brace.square"
      ],
      "settings": {
        "foreground": "#c0c0c0"
      }
    },
    {
      "scope": [
        "constant.language",
        "constant.other.placeholder",
      ],
      "settings": {
        "foreground": "#73162d"
      }
    },
    {
      "scope": [
        "keyword",
        "storage",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.wordlike",
        "keyword.operator.arithmetic.mod.gml",
        "storage.type.gml",
        "storage.type.function.gml",
        "function.defaultLibrary",
        "support.function.gml",
        "entity.name.operator.custom-literal",
        "entity.name.function",
      ],
      "settings": {
        "foreground": "#ffb871"
      }
    },
    {
      "scope": [
        "variable.other.local.gml",
      ],
      "settings": {
        "foreground": "#b2b1ff"
      }
    },
    {
      "scope": [
        "property.static",
        "variable.parameter.gml",
      ],
      "settings": {
        "foreground": "#fff899"
      }
    },
    {
      "scope": [
        "storage.type.annotation",
        "entity.name.type.instance.gml",
        "support.type"
      ],
      "settings": {
        "foreground": "#6771e1"
      }
    },
    {
      "scope": [
        "keyword.other.documentation",
        "keyword.documentation.tag",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#b871ff"
      }
    },
    {
      "scope": [
        "variable.other.gml",
      ],
      "settings": {
        "foreground": "#72bfff"
      }
    },
    {
      "scope": [
        "variable.other.global.gml",
      ],
      "settings": {
        "foreground": "#ff7eff",
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "foreground": "#5b995b",
        "fontStyle": "italic bold"
      }
    },
  ]
}